{
  "manifest_version": 3,
  "name": "Scribed Downloader",
  "version": "1.0",
  "description": "Scribed Downloader",
  "action": {
    "default_icon": "icon.png",
    "default_title": "Scribed Downloader"
  },
  "homepage_url": "https://github.com/ArielParra/Scribed_downloader",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.scribd.com//*"],
      "js": [
      "content.js"
      ]
    }
  ]
}

